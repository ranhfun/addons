<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

        <p>
        	<t:form>
            <t:select t:id="color" model="literal:Red,Green,Blue"/>
            <t:any t:id="output" t:mixins="snack/controlledselecttext" context="colorId" controller="color">${output}</t:any>
            <t:any t:id="output2" t:mixins="snack/controlledselecttext" context="colorId" controller="color">${output}</t:any>
            </t:form>
        </p>
</html>
